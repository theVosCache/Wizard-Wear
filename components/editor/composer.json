{
    "name": "thevoscache/editor",
    "description": "Livewire block based editor",
    "keywords": [
        "theVosCache",
        "laravel",
        "editor"
    ],
    "homepage": "https://github.com/thevoscache/editor",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthijs De vos",
            "email": "m.devos96@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^11.0",
        "illuminate/support": "^11.0",
        "livewire/livewire": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "TheVosCache\\Editor\\": "src/",
            "TheVosCache\\Editor\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheVosCache\\Editor\\EditorServiceProvider"
            ],
            "aliases": {
                "Editor": "TheVosCache\\Editor\\Facades\\Editor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/framework": "^11",
        "symplify/easy-coding-standard": "^11"
    }
}
