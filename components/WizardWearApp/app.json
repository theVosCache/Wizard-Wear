{
  "expo": {
    "scheme": "wizard-wear",
    "name": "WizardWearApp",
    "slug": "WizardWearApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/parchment-background.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "nl.m_dv.wizard_wear"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
